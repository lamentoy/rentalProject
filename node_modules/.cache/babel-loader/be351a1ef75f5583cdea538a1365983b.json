rch2.getPlace()\n      console.log(googleLocation)\n      this.googleLocation2=googleLocation.formatted_address\n    })\n\n\n\n  },\n  data(){\n    return{\n      locationName:\"\",\n      locationName1:\"\",\n      locationType:\"\",\n      searchLocations:[],\n      searchLocations2:[],\n      LocationTypes:[],\n      googleLocation:\"\",\n      googleLocation2:\"\"\n    }\n  },\n  methods:{\n    addLocations(){\n      this.searchLocations.push(this.googleLocation)\n      this.locationName=\"\"\n\n\n    },\n    addLocations2(){\n      this.searchLocations2.push(this.googleLocation2)\n      this.locationName1=\"\"\n\n\n    },\n    addLocationTypes(){\n      this.LocationTypes.push(this.locationType)\n      this.locationType=\"\"\n    }\n  }\n\n}\n</script>\n\n<style lang=\"less\" scoped>\n .first_Page {\n    width:100%;\n    margin:20px 0;\n    height:80%;\n    position: fixed;\n    overflow: scroll;\n    .clickable{\n        cursor: pointer;\n    }\n    .mg-bt-10{\n        margin-bottom: 10px;\n    }\n    .operations{\n        display: flex;\n        justify-content: space-between;\n    }\n    .el-input{\n        width:100%;\n        margin:20px 0;\n    }\n     .steps{\n       margin: 20px 0;\n       border:1px solid orange;\n       div{\n         padding:10px;\n         &:first-child{\n         background-color: orange;\n       }\n       }\n    \n     }\n  }\n</style>"]},"metadata":{},"sourceType":"module"}